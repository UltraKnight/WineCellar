<div class="container-fluid container-general container-wine text-light text-center">
    <h3 class="pt-3 mb-3 shaded">Achievements</h3>
    <div class="row">
        <div class="opened col-md-10 offset-md-1 mt-4 text-light">
            {{#each achievements}}
              <div class="card bg-transparent" style="width: 18rem;">
                <img src="{{imageURL}}" class="card-img-top m-auto" alt="{{name}}" style="max-width: 150px;">
                <div class="card-body">
                  <h5 class="card-title shaded">{{name}}</h5>
                  <p class="card-text shaded">{{description}}</p>
                </div>
              </div>
            {{/each}}
        </div>
    </div>
</div>